{
    "name": "arunfung/php-apollo",
    "description": "a apollo client for php.",
    "keywords": ["apollo", "php", "php-apollo", "apollo-client"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/arunfung/php-apollo/issues",
        "source": "https://github.com/arunfung/php-apollo"
    },
    "authors": [
        {
            "name": "Arun Fung",
            "email": "arunfung@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "~7.1",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "ArunFung\\PhpApollo\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
